<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema elementFormDefault="qualified"
    targetNamespace="http://www.opengis.net/gml"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:import schemaLocation="http://schemas.opengis.net/gml/3.1.1/base/gml.xsd" namespace="http://www.opengis.net/gml"/>
    <xs:element xmlns:gml="http://www.opengis.net/gml" name="TestSimple" type="gml:TestSimpleType" substitutionGroup="gml:_Feature"/>
    <xs:complexType name="TestSimpleType">
        <xs:complexContent>
            <xs:extension xmlns:gml="http://www.opengis.net/gml" base="gml:AbstractFeatureType" >
                <xs:sequence>
                    <xs:element name="ID"                  maxOccurs="1" minOccurs="1" nillable="true" type="xs:integer"/>
                    <xs:element name="attString"           maxOccurs="1" minOccurs="1" nillable="true" type="xs:string"/>
                    <xs:element name="attShort"            maxOccurs="1" minOccurs="1" nillable="true" type="xs:short"/>
                    <xs:element name="attInteger"          maxOccurs="1" minOccurs="1" nillable="true" type="xs:integer"/>
                    <xs:element name="attLong"             maxOccurs="1" minOccurs="1" nillable="true" type="xs:long"/>
                    <xs:element name="attDouble"           maxOccurs="1" minOccurs="1" nillable="true" type="xs:double"/>
                    <xs:element name="attDecimal"          maxOccurs="1" minOccurs="1" nillable="true" type="xs:decimal"/>
                    <xs:element name="attDate"             maxOccurs="1" minOccurs="1" nillable="true" type="xs:date"/>
                    <xs:element name="attDateTime"         maxOccurs="1" minOccurs="1" nillable="true" type="xs:dateTime"/>
                    <xs:element name="attBoolean"          maxOccurs="1" minOccurs="1" nillable="true" type="xs:boolean"/>
                    <xs:element name="geomPoint"           maxOccurs="1" minOccurs="1" nillable="true" type="gml:Point"/>
                    <xs:element name="geomMultiPoint"      maxOccurs="1" minOccurs="1" nillable="true" type="gml:MultiPoint"/>
                    <xs:element name="geomLine"            maxOccurs="1" minOccurs="1" nillable="true" type="gml:Curve"/>
                    <xs:element name="geomMultiLine"       maxOccurs="1" minOccurs="1" nillable="true" type="gml:CompositeCurve"/>
                    <xs:element name="geomPolygon"         maxOccurs="1" minOccurs="1" nillable="true" type="gml:Polygon"/>
                    <xs:element name="geomMultiPolygon"    maxOccurs="1" minOccurs="1" nillable="true" type="gml:MultiGeometry"/>
                    <xs:element name="geomMultiGeometry"   maxOccurs="1" minOccurs="1" nillable="true" type="gml:MultiGeometry"/>
                    <xs:element name="geomAnyGeometry"     maxOccurs="1" minOccurs="1" nillable="true" type="gml:GeometryPropertyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>
