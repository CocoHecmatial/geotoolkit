<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<gml:GeographicCRS gml:id="coordinate-reference-system" xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:gmx="http://www.isotc211.org/2005/gmx">
  <gml:identifier codeSpace="">WGS84(DD)</gml:identifier>
  <gml:ellipsoidalCS>
    <gml:EllipsoidalCS gml:id="coordinate-system">
      <gml:identifier codeSpace="">Géodésique 2D</gml:identifier>
      <gml:axis>
        <gml:CoordinateSystemAxis gml:id="coordinate-system-axis" uom="°">
          <gml:identifier codeSpace="">Geodetic longitude</gml:identifier>
          <gml:axisAbbrev>λ</gml:axisAbbrev>
          <gml:axisDirection codeSpace="EPSG">east</gml:axisDirection>
        </gml:CoordinateSystemAxis>
      </gml:axis>
      <gml:axis>
        <gml:CoordinateSystemAxis gml:id="coordinate-system-axis" uom="°">
          <gml:identifier codeSpace="">Geodetic latitude</gml:identifier>
          <gml:axisAbbrev>φ</gml:axisAbbrev>
          <gml:axisDirection codeSpace="EPSG">north</gml:axisDirection>
        </gml:CoordinateSystemAxis>
      </gml:axis>
    </gml:EllipsoidalCS>
  </gml:ellipsoidalCS>
  <gml:geodeticDatum>
    <gml:GeodeticDatum gml:id="datum">
        <gml:identifier codeSpace="OGC">WGS84</gml:identifier>
        <gml:realizationEpoch/>
    </gml:GeodeticDatum>
  </gml:geodeticDatum>
</gml:GeographicCRS>
