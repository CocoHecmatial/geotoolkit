<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<gml:TimeEdge xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xmlns:gml="http://www.opengis.net/gml/3.2" 
              xsi:schemaLocation = "http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/temporalTopology.xsd"  
              gml:id="auto-tripleedge">
    <gml:identifier codeSpace="AUTO">Triple Edge</gml:identifier>
    <gml:name codeSpace="AUTO">Edge</gml:name>
    <gml:start>
        <gml:TimeNode gml:id="auto-nodebeginidentifier">
            <gml:identifier codeSpace="AUTO">Node begin identifier</gml:identifier>
            <gml:name codeSpace="AUTO">NodeBegin</gml:name>
            <gml:position>
                <gml:TimeInstant gml:id="auto-instantbegin">
                    <gml:identifier codeSpace="AUTO">Instant begin</gml:identifier>
                    <gml:name codeSpace="AUTO">Instant Begin</gml:name>
                    <gml:timePosition>2014-10-20T15:50:46.273+02:00</gml:timePosition>
                </gml:TimeInstant>
            </gml:position>
        </gml:TimeNode>
    </gml:start>
    <gml:end>
        <gml:TimeNode gml:id="auto-nodeendidentifier">
            <gml:identifier codeSpace="AUTO">Node end identifier</gml:identifier>
            <gml:name codeSpace="AUTO">NodeEnd</gml:name>
            <gml:position>
                <gml:TimeInstant gml:id="auto-instantend">
                    <gml:identifier codeSpace="AUTO">Instant End</gml:identifier>
                    <gml:name codeSpace="AUTO">Instant End</gml:name>
                    <gml:timePosition>2014-10-20T15:50:46.273+02:00</gml:timePosition>
                </gml:TimeInstant>
            </gml:position>
        </gml:TimeNode>
    </gml:end>
</gml:TimeEdge>
